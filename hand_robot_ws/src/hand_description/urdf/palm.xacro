      </visual>
      <collision>
        <geometry>
          <mesh filename="package://hand_description/meshes/SG90_arm.stl" scale="0.001 0.001 0.001"/>
        </geometry>
        <origin xyz="0 0 0" rpy="0 0 0"/>
      </collision>
      <xacro:cylinder_inertia mass="0.5" radius="0.08" length="0.25"/>
